                </element>
                <element target="body2" x="0" y="5" z="1">
                    <type>body2</type>
                    <image>demo/LB/U6/L3-68/images/DRAMBIWB377f.png</image>
                    <thumb>demo/LB/U6/L3-68/images/DRAMBIWB377fa.png</thumb>
                    <sound>giggle</sound> 
                </element>
            </elements>
        </screen>
    </screens>
    <sounds>
        <sound>
        <id>intro</id>
        <start>0.96</start>
        <stop>9.04</stop>
