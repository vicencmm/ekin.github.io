<?xml version="1.0" encoding="UTF-8"?>
<!--

By defining a config variable in the root of the xml you can make
the elements draggable. This enables you to determine the position
of each element through the output in the console.

 <game type="make-picture" config="1">

-->
<game type="make-picture" config="0" audio="demo/LA/U5/L3-47/audio/audio.mp3,demo/LA/U5/L3-47/audio/audio.ogg">
    <title>Dress Daisy in her superhero costume.</title>

    <screens random="true">
        <screen>
		<perRow>4</perRow>
            <character width="350" height="350">
                <image>demo/LA/U5/L3-47/images/DRAMAIWB156.png</image>
                <targets>
                    <target x="-1" y="0" id="wrist1"></target>
                    <target x="-1" y="0" id="head"></target>
                    <target x="-1" y="0" id="body"></target>
                    <target x="-1" y="0" id="back"></target>
                </targets>
                <!-- <animation x="0" y="0" z="0" >
                    <source>img/stars.gif</source>
                </animation> -->
            </character>
            <elements>

                <element target="head" x="0" y="0" z="10">
                    <type>head</type>
                    <image>demo/LA/U5/L3-47/images/DRAMAIWB156a.png</image>
                    <thumb>demo/LA/U5/L3-47/images/DRAMAIWB156d.png</thumb>
                    <sound>DRAM_GIGGLE_DAISY</sound> 
                </element>
                <element target="head" x="0" y="0" z="10">
                    <type>head</type>
                    <image>demo/LA/U5/L3-47/images/DRAMAIWB401a.png</image>
                    <thumb>demo/LA/U5/L3-47/images/DRAMAIWB401g.png</thumb>
                    <sound>DRAM_GIGGLE_DAISY</sound> 
                </element>
                 <element target="head" x="0" y="0" z="10">
                    <type>head</type>
                    <image>demo/LA/U5/L3-47/images/DRAMAIWB003a.png</image>
                    <thumb>demo/LA/U5/L3-47/images/DRAMAIWB003g.png</thumb>
                    <sound>DRAM_GIGGLE_DAISY</sound> 
                </element>
				<element target="head" x="0" y="0" z="10">
                    <type>head</type>
                    <image>demo/LA/U5/L3-47/images/DRAMAIWB274abc.png</image>
                    <thumb>demo/LA/U5/L3-47/images/DRAMAIWB274ghi.png</thumb>
                    <sound>DRAM_GIGGLE_DAISY</sound> 
